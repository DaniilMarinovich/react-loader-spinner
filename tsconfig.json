{
	"compilerOptions": {
		"target": "es6",
    "jsx": "react",
		"module": "commonjs",
		"declaration": true,
		"sourceMap": true,
		"outDir": "lib",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	},
  "exclude": ["docs"]
}
